#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/list-it@1.3.10/node_modules/list-it/bin/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/list-it@1.3.10/node_modules/list-it/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/list-it@1.3.10/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/list-it@1.3.10/node_modules/list-it/bin/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/list-it@1.3.10/node_modules/list-it/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/list-it@1.3.10/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../list-it@1.3.10/node_modules/list-it/bin/listit.js" "$@"
else
  exec node  "$basedir/../../../../../list-it@1.3.10/node_modules/list-it/bin/listit.js" "$@"
fi
