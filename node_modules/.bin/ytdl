#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/ytdl@1.4.1/node_modules/ytdl/bin/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/ytdl@1.4.1/node_modules/ytdl/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/ytdl@1.4.1/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/ytdl@1.4.1/node_modules/ytdl/bin/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/ytdl@1.4.1/node_modules/ytdl/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/ytdl@1.4.1/node_modules:/Users/nicolaerusan/Projects/toolkit/langchain-demos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ytdl/bin/ytdl.js" "$@"
else
  exec node  "$basedir/../ytdl/bin/ytdl.js" "$@"
fi
